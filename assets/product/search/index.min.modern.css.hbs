}

.base-input-area .placeholder{
  color:#2c2c2c;
  left:{{append (add 16 theme.settings.input_border_thickness) 'px'}};
  opacity:.6;
  pointer-events:none;
  position:absolute;
  right:0;
  top:50%;
  transform:translateY(-50%);
  transform-origin:left top;
  transition:transform .3s ease;
  word-break:break-word;
  z-index:4;
}

.base-input-area.is-textarea{
  padding-top:{{append (add 24 theme.settings.input_border_thickness) 'px'}};
}
